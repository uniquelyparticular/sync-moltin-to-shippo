{
  "name": "demo-sync-moltin-to-zendesk",
  "alias": "particular-demo-sync-moltin-to-zendesk.now.sh",
  "env": {
    "NODE_ENV": "production",
    "MOLTIN_CLIENT_ID": "@demo-moltin-client-id",
    "MOLTIN_CLIENT_SECRET": "@demo-moltin-client-secret",
    "MOLTIN_WEBHOOK_SECRET": "@demo-moltin-webhook-secret",
    "AWS_ACCESS_KEY_ID": "@demo-aws-access-key-id",
    "AWS_SECRET_ACCESS_KEY": "@demo-aws-secret-access-key",
    "AWS_REGION": "@demo-aws-region",
    "SHIPPO_PRIVATE_KEY": "@demo-shippo-private-key",
    "EMAIL_FROM": "@demo-email-from"
  },
  "routes": [{ "src": "/(.*)", "dest": "/src" }],
  "builds": [
    {
      "src": "src/*.js",
      "use": "@now/node"
    }
  ]
}
